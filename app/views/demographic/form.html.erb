<%= form_tag(demographic_get_url, method: 'get', class: 'form',) do %>
    <fieldset>
      payer_name:            <%= text_field_tag(:payer_name, 'Aetna') %><br>
      payer_id:              <%= text_field_tag(:payer_id, '000001') %><br>
      provider_last_name:    <%= text_field_tag(:provider_last_name, 'Last') %><br>
      provider_first_name:   <%= text_field_tag(:provider_first_name, 'First') %><br>
      provider_npi:          <%= text_field_tag(:provider_npi, '12345678') %><br>
      member_id:             <%= text_field_tag(:member_id, '12345678') %><br>
      member_last_name:      <%= text_field_tag(:member_last_name, 'Austen') %><br>
      member_first_name:     <%= text_field_tag(:member_first_name, 'Jane') %><br>
      member_dob:            <%= text_field_tag(:member_dob, '1955-12-14') %><br>
      member_gender:         <%= text_field_tag(:member_gender, 'male') %><br>
      internal_id:           <%= text_field_tag(:internal_id, '19551214') %>
    </fieldset>
    <%= submit_tag('Get!') %>
<% end %>